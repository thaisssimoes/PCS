package exercicio2;
import static MaiorElemento.maiorElemento;

public class Exercicio2 {
	public static void main(String args[]){
		int[] vetor = new int[]{1,52,83,4,5,16,7,86,9,10};
		int elemMax;
		for(int i =0; i < vetor.length; i++){
			elemMax= maiorElemento(vetor, i);

		}
		
		System.out.println("O maior elemento do array e: " + elemMax);
	}
	
}
