package exercicio2;

public class Exercicio2 {
	public static void main(String args[]){
		int[] vetor = new int[]{1,52,83,4,5,156,7,86,9,10};
		int elemMax=0;
		
		for(int i =0; i < vetor.length; i++){
			elemMax= MaiorElemento.maiorElemento(vetor, i);
		}
		
		System.out.println("O maior elemento do array e: " + elemMax);
	}
	
}
